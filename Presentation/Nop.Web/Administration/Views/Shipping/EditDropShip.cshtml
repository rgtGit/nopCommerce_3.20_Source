@model ShippingDropModel
@{
    //page title
    ViewBag.Title = T("Admin.Configuration.Shipping.DropShip.EditParameters").Text;
}
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
            <img src="@Url.Content("~/Administration/Content/images/ico-configuration.png")" alt="" />
            @T("Admin.Configuration.Shipping.DropShip.EditDropSetting")
        </div>
        <div class="options">
            <input type="submit" name="save" class="t-button" value="@T("Admin.Common.Save")" />
        </div>
    </div>
    @Html.Partial("_CreateOrUpdateShipDrop", Model)
}

