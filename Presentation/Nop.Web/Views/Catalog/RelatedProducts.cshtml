@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <div class="container">
        <strong>@T("Products.RelatedProducts")</strong>
            <div class="row">
                <div class="col-md-12">
                    <table class="table">
                        <tr>
                            <th></th>
                            <th>Part #</th>
                            <th>Part Name</th>
                            <th>Price</th>
                            <th>Qty</th>
                            <th></th>
                        </tr>
                        @foreach (var item in Model)
                        {
                            <div class="item-box">@Html.Partial("_ProductBox", item)</div>
                        }
                    </table>    
             </div>
          </div>
    </div>        
}